{
  "name": "lacona-ifttt",
  "version": "1.0.0",
  "description": "Lacona Addon for integrating with anything on IFTTT (via the Maker Channel)",
  "main": "lib/index.js",
  "lacona": {
    "title": "IFTTT",
    "description": "Do anything on IFTTT (by using the Maker Channel)",
    "config": "config.json"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "lacona",
    "lacona-addon"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/lacona-ifttt.git"
  },
  "dependencies": {
    "elliptical": "^2.0.0",
    "lacona-api": "^1.4.0",
    "lacona-phrases": "^1.1.0",
    "lodash": "^4.16.4",
    "node-fetch": "^1.6.3"
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-cli": "^6.14.0",
    "babel-preset-es2015-node": "^6.1.1",
    "rimraf": "^2.5.4"
  },
  "babel": {
    "presets": [
      "es2015-node"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  }
}
